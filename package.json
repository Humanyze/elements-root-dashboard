    "rc-tooltip": "^4.0.0-alpha.3",
    "react": "^16.12.0",
    "react-app-rewired": "^2.1.5",
    "react-datepicker": "^2.11.0",
    "react-dates": "^21.5.1",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-moment-proptypes": "^1.7.0",
    "react-pose": "^4.0.10",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-scroll-into-view-if-needed": "^2.1.7",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-axios-middleware": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.2.0",
    "redux-thunk": "^2.3.0",
    "require-context.macro": "^1.2.2",
    "reselect": "^4.0.0",
    "sass-loader": "^8.0.1",
    "storybook": "^5.3.0",
    "storybook-react-router": "^1.0.8",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "start": "PORT=3001 react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "test:debug": "react-app-rewired --inspect-brk test --runInBand --no-cache --env=jsdom",
    "eject": "react-scripts eject",
    "storybook": "./cscripts/storybook",
    "storybook:container": "start-storybook -p 9003 -c .storybook -s ./public",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "common-docs": "jsdoc -c ./src/components/common/jsdoc.conf ./src/components/common/ -r -d ./src/components/common/docs"
  },
  "eslintConfig": {
    "extends": "eslint-config-humanyze"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/**/*.story.js",
      "!src/tests/**"
    ],
    "moduleNameMapper": {
      "^Common(.*)$": "<rootDir>/src/components/common$1",
      "^appPackageJson": "<rootDir>/package.json",
      "^Src(.*)$": "<rootDir>/src$1",
      "^Public(.*)$": "<rootDir>/public$1",
      "^Assets(.*)$": "<rootDir>/src/assets$1",
      "^Redux([a-z/]*)$": "<rootDir>/src/redux$1",
      "^ReduxCommon(.*)$": "<rootDir>/src/elements-web-common/redux$1",
      "^Utils(.*)$": "<rootDir>/src/elements-web-common/assets/utils$1",
      "^TestUtils(.*)$": "<rootDir>/src/tests$1",
      "^ElementsWebCommon(.*)$": "<rootDir>/src/elements-web-common$1",
      "^RouterPaths": "<rootDir>/src/routerPaths.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
